    "semanticHighlighting": false,
    "semanticTokenColors": {
        "union.declaration": "#e6662f",
        "class": "#5d97b4",
        "function.declaration": "#e6662f",
        "keyword": "#586583",
    },
    "tokenColors": [
        /*
        ██   ██ ███████ ██    ██ ██     ██  ██████  ██████  ██████  ███████ 
        ██  ██  ██       ██  ██  ██     ██ ██    ██ ██   ██ ██   ██ ██      
        █████   █████     ████   ██  █  ██ ██    ██ ██████  ██   ██ ███████ 
        ██  ██  ██         ██    ██ ███ ██ ██    ██ ██   ██ ██   ██      ██ 
        ██   ██ ███████    ██     ███ ███   ██████  ██   ██ ██████  ███████
        */
        {
            "name": "Base Keywords",
            "scope": [
                "constant.language.json",
                "keyword",
                "meta.function",
                "meta.tag.metadata.doctype.html",
                "punctuation.brackets.curly.rust",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "punctuation.section.block",
                "storage"
            ],
            "settings": {
                "foreground": "#626e8a",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Labels",
            "scope": [
                "entity.name.label",
                "storage.type.modifier.access.control"
            ],
            "settings": {
                "foreground": "#475972",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS Keyword Values",
            "scope": [
                "support.constant.property-value.css",
                "support.constant.color"
            ],
            "settings": {
                "foreground": "#c0c0c0",
                "fontStyle": "bold"
            }
        },
    /*
        ██    ██  █████  ██████  ██  █████  ██████  ██      ███████ ███████ 
        ██    ██ ██   ██ ██   ██ ██ ██   ██ ██   ██ ██      ██      ██      
        ██    ██ ███████ ██████  ██ ███████ ██████  ██      █████   ███████ 
         ██  ██  ██   ██ ██   ██ ██ ██   ██ ██   ██ ██      ██           ██ 
          ████   ██   ██ ██   ██ ██ ██   ██ ██████  ███████ ███████ ███████ 
        */
        {
            "name": "Global Variables",
            "scope": [
                "entity.name.namespace",
                "entity.name.scope-resolution",
                "punctuation.separator.scope-resolution",
                "support.constant.math",
                "variable.language.special.self.python",
                "variable.language.super",
                "variable.language.this",
            ],
            "settings": {
                "foreground": "#8398c5",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "entity.name.tag.css",
                "meta.body",
                "meta.function-call.arguments",
                "new.expr",
                "variable.other",
                "variable.parameter",
            ],
            "settings": {
                "foreground": "#bebebe",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Keys & Members",
            "scope": [
                "entity.name.function.member",
                "entity.other.attribute-name.id.css",
                "markup.list",
                "meta.attribute.python",
                "meta.attribute.rust",
                "meta.member.access",
                "meta.object.member",
                "support.variable.property",
                "variable.other.constant.property",
                "variable.other.object.property",
                "variable.other.property",
            ],
            "settings": {
                "foreground": "#dfd4cdce",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Calls",
            "scope": [
                "entity.name.function.call",
                "meta.function-call",
                "meta.function.call",
                "markup.quote.markdown",
            ],
            "settings": {
                "foreground": "#a8a8a8",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Declarations",
            "scope": [
                "entity.name.label",
                "entity.other.attribute-name.html",
                "entity.other.attribute-name",
                "literal.key",
                "meta.attribute.html",
                "meta.definition.property",
                "meta.definition.variable",
                "meta.object-literal.key",
                "meta.object.member",
                "support.type.property-name.css",
            ],
            "settings": {
                "foreground": "#c9ba9fe5",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Block Declarations",
            "scope": [
                "entity.name.function.constructor",
                "entity.name.function.definition",
                "entity.name.module",
                "entity.name.type.class",
                "keyword.other.operator.overload",
                "meta.function.definition.rust",
                "meta.definition.function",
                "meta.definition.method",
                "meta.head.function.definition.special.operator-overload",
                "support.type.property-name.json",
            ],
            "settings": {
                "foreground": "#fc7f4a",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Types & Storage",
            "scope": [
                "cast.expr",
                "entity.name.type",
                "entity.other.inherited-class",
                "fenced_code.block.language.markdown",
                "markup.heading.markdown",
                "meta.arguments.decltype",
                "meta.return.type",
                "meta.template.definition",
                "meta.type",
                "keyword.operator.dereference",
                "keyword.operator.borrow",
                "storage.type.built-in.primitive",
                "storage.type.primitive",
                "storage.type.template.argument",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#887fda",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Defined Types",
            "scope": [
                "meta.tag",
            ],
            "settings": {
                "foreground": "#8675c2d7",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Tag Selector",
            "scope": [
                "entity.name.tag.css"
            ],
            "settings": {
                "foreground": "#adadad",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS Class Selector",
            "scope": [
                "entity.other.attribute-name.class.css"
            ],
            "settings": {
                "foreground": "#bbbbbb",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS Meta Selector",
            "scope": [
                "meta.selector.css"
            ],
            "settings": {
                "foreground": "#a5a5a5",
                "fontStyle": "italic"
            }
        },
        /*
        ███████ ████████ ██████  ██ ███    ██  ██████  ███████ 
        ██         ██    ██   ██ ██ ████   ██ ██       ██      
        ███████    ██    ██████  ██ ██ ██  ██ ██   ███ ███████ 
             ██    ██    ██   ██ ██ ██  ██ ██ ██    ██      ██ 
        ███████    ██    ██   ██ ██ ██   ████  ██████  ███████ 
        */
        {
