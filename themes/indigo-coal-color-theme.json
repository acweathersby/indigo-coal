        /*
         ██████  ██████  ███████ ██████   █████  ████████  ██████  ██████  ███████ 
        ██    ██ ██   ██ ██      ██   ██ ██   ██    ██    ██    ██ ██   ██ ██      
        ██    ██ ██████  █████   ██████  ███████    ██    ██    ██ ██████  ███████ 
        ██    ██ ██      ██      ██   ██ ██   ██    ██    ██    ██ ██   ██      ██ 
         ██████  ██      ███████ ██   ██ ██   ██    ██     ██████  ██   ██ ███████ 
        */
        {
            "name": "Operator",
            "scope": [
                "constant.language.import-export-all.js",
                "keyword.operator",
                "operator",
                "storage.type.function.arrow",
            ],
            "settings": {
                "foreground": "#5d97b4",
                "fontStyle": "italic"
            }
        },
        /*
        ██████  ██    ██ ███    ██  ██████ ████████ ██    ██  █████  ████████ ██  ██████  ███    ██ 
        ██   ██ ██    ██ ████   ██ ██         ██    ██    ██ ██   ██    ██    ██ ██    ██ ████   ██ 
        ██████  ██    ██ ██ ██  ██ ██         ██    ██    ██ ███████    ██    ██ ██    ██ ██ ██  ██ 
        ██      ██    ██ ██  ██ ██ ██         ██    ██    ██ ██   ██    ██    ██ ██    ██ ██  ██ ██ 
        ██       ██████  ██   ████  ██████    ██     ██████  ██   ██    ██    ██  ██████  ██   ████
        */
        {
            "name": "Punctuation",
            "scope": [
                "entity.name.function.macro",
                "keyword.control.directive",
                "markup",
                "meta.arrow",
                "meta.class",
                "meta.method.declaration",
                "meta.object-binding-pattern-variable.ts",
                "punctuation",
            ],
            "settings": {
                "foreground": "#78737e",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Punctuation Member Accessor",
            "scope": [
                "punctuation.accessor",
                "punctuation.separator.key-value",
            ],
            "settings": {
                "foreground": "#848b8f",
                "fontStyle": "bold"
            }
        },
        /*
        ██████  ██████   █████   ██████ ██   ██ ███████ ████████ ███████ 
        ██   ██ ██   ██ ██   ██ ██      ██  ██  ██         ██    ██      
        ██████  ██████  ███████ ██      █████   █████      ██    ███████ 
        ██   ██ ██   ██ ██   ██ ██      ██  ██  ██         ██         ██ 
        ██████  ██   ██ ██   ██  ██████ ██   ██ ███████    ██    ███████
        */
        {
            "name": "HTML Brackets",
            "scope": [
                "punctuation.definition.tag",
                "punctuation.section.property-list"
            ],
            "settings": {
                "foreground": "#7f77ccd7",
                "fontStyle": "bold"
            }
        },
        {
            "name": "String Template Brackets",
            "scope": [
                "punctuation.definition.template-expression",
            ],
            "settings": {
                "foreground": "#5b7a61",
                "fontStyle": "bold"
            }
    },
        {
            "name": "Brackets",
            "scope": [
                "meta.brace.css",
                "punctuation.section.arguments.begin.bracket",
                "punctuation.section.arguments.end.bracket",
                "punctuation.section.parameters.begin.bracket",
                "punctuation.section.parameters.end.bracket",
            ],
            "settings": {
                "foreground": "#5d97b4",
                "fontStyle": "bold"
            }
        },
        /*
         ██████  ████████ ██   ██ ███████ ██████  
        ██    ██    ██    ██   ██ ██      ██   ██ 
        ██    ██    ██    ███████ █████   ██████  
        ██    ██    ██    ██   ██ ██      ██   ██ 
         ██████     ██    ██   ██ ███████ ██   ██
        */
        {
            "name": "Plain Text",
            "scope": [
                "text",
                "text.html.markdown",
                "text.html.derivative",
            ],
            "settings": {
                "foreground": "#c0c0c0",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Bold",
            "scope": [
                "markup.bold.markdown",
                "punctuation.definition.bold.markdown"
