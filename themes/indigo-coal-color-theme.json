            "scope": [
                "string",
                "source.ignore",
                "punctuation.definition.string",
                "markup.underline.link.image.markdown",
                "meta.link.inline.markdown"
            ],
            "settings": {
                "foreground": "#6ea363",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Raw String",
            "scope": [
                "string.unquoted",
                "markup.inline",
            ],
            "settings": {
                "foreground": "#62aeda",
                "fontStyle": "italic"
            }
        },
        /*
        ██      ██ ████████ ███████ ██████   █████  ██      ███████ 
        ██      ██    ██    ██      ██   ██ ██   ██ ██      ██      
        ██      ██    ██    █████   ██████  ███████ ██      ███████ 
