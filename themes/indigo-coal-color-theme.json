        /*
         ██████  ██████  ███    ███ ███    ███ ███████ ███    ██ ████████ ███████ 
        ██      ██    ██ ████  ████ ████  ████ ██      ████   ██    ██    ██      
        ██      ██    ██ ██ ████ ██ ██ ████ ██ █████   ██ ██  ██    ██    ███████ 
        ██      ██    ██ ██  ██  ██ ██  ██  ██ ██      ██  ██ ██    ██         ██ 
         ██████  ██████  ██      ██ ██      ██ ███████ ██   ████    ██    ███████
        */ {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment",
                "markup.heading.setext"
            ],
            "settings": {
                "foreground": "#7E7A8FBD",
                "fontStyle": "bold"
            }
